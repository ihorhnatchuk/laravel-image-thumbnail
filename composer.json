{
    "name": "ihorhnatchuk/laravel-image-thumbnail",
    "description": "Image manipulation library for Laravel 8 based on Imagine and inspired by Croppa for easy url based manipulation",
    "keywords": ["laravel","image","imagick","gd","imagine","watermark","gmagick","thumbnail"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ihorhnatchuk",
            "email": "ihorhnatchuk@gmail.com",
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "^6.18|^7.0|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "imagine/imagine": "0.6.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "orchestra/testbench": "^4.9",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-0": {
            "Folklore\\Image\\": "src/",
            "Folklore\\Image\\Tests": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Folklore\\Image\\ImageServiceProvider"
            ],
            "aliases": {
                "Thumbnail": "Folklore\\Image\\Facades\\Thumbnail"
            }
        }
    }
}
